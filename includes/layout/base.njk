---
lang: en
---

<!doctype html>
<html lang="{{ lang }}">
<meta charset=utf-8>
<title>Deniz Akşimşek</title>
<meta name=viewport content=width=device-width,initial-scale=1.0>

<!--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+
    Preloading fonts 
    +-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-->

{% for font in [
	"/assets/fonts/InriaSans-Regular.ttf",
	"/assets/fonts/InriaSans-Bold.ttf",
	"/assets/fonts/InriaSans-Italic.ttf"
	] 
%}
<link rel=preload href="{{ font }}" as=font crossorigin=anonymous>
{% endfor %}

<!--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+
    Styles
    +-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-->

<link rel=stylesheet href=/styles/styles.css>
<link rel=stylesheet href=/styles/prism-synthwave84.css media=none
      onload=media='all';onload=null>

<!--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+
    Theme
    +-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-->

<meta name=theme-color content=#e13200>
<link rel="shortcut icon" href=/assets/me.jpeg>

<!--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+
    IndieAuth 
    +-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-->

<link rel="authorization_endpoint" href="https://indieauth.com/auth">
<link rel="token_endpoint" href="https://tokens.indieauth.com/token">

<!--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+
    Webmention
    +-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-->

<link rel="webmention" href="https://webmention.io/www.denizaksimsek.com/webmention">
<link rel="pingback" href="https://webmention.io/www.denizaksimsek.com/xmlrpc">

<!--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+
    Cards 
    +-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-->

{% if collections.entries | getCollectionItem(page) %}
    <meta property=og:title content="{{ tagline }}">
    <meta property=og:url content=https://denizaksimsek.com{{ page.url }}>
    <meta property=og:type content=article>
    {% if uPhoto %}
    	<meta property=og:image content="{{ uPhoto }}">
    {% endif %}
{% endif %}

<meta name=twitter:card content="summary{{ '_large_image' if uPhoto }}">
<meta name=twitter:site content=@DenizAksimsek>
<meta name=twitter:creator content=@DenizAksimsek>

<!--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-->
    <body>
<!--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-->

{{ content | safe }}
